  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/underscore": "^1.10.12",
    "@wdio/cli": "^6.3.4",
    "@wdio/cucumber-framework": "^6.3.0",
    "@wdio/local-runner": "^6.3.4",
    "@wdio/spec-reporter": "^6.3.0",
    "@wdio/sync": "^6.3.3",
    "chai": "^4.2.0",
