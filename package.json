  "version": "1.0.0",
  "description": "test automation framework for justeat website using typescript,cucumberJS and webdriverIO",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/rimraf 'jsonReports/*.json' ; ./node_modules/.bin/wdio wdio.conf.js; node cucumber-html-report.js",
    "cucumber": "./node_modules/.bin/cucumber-js features/*.feature --require 'step-definitions/**/*.ts' --require-module ts-node --require-module cucumber-pretty"
  },
